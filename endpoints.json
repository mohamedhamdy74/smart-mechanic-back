{
  "auth_register": {
    "method": "POST",
    "url": "/auth/register",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "role": "user",
      "name": "John Doe",
      "email": "john@example.com",
      "password": "password123",
      "phone": "1234567890",
      "location": "New York"
    },
    "expected_response": {
      "message": "User registered"
    }
  },
  "auth_login": {
    "method": "POST",
    "url": "/auth/login",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "email": "john@example.com",
      "password": "password123"
    },
    "expected_response": {
      "token": "jwt_token_here"
    }
  },
  "users_get_all": {
    "method": "GET",
    "url": "/users?page=1&limit=10",
    "headers": {
      "Authorization": "Bearer jwt_token_here"
    },
    "expected_response": {
      "users": [
        {
          "role": "user",
          "name": "John Doe",
          "email": "john@example.com",
          "phone": "1234567890",
          "location": "New York",
          "_id": "user_id",
          "createdAt": "2023-01-01T00:00:00.000Z",
          "updatedAt": "2023-01-01T00:00:00.000Z"
        }
      ],
      "total": 1,
      "page": 1,
      "pages": 1
    }
  },
